<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
    <event>
        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.2636.4.23</mevalue>
            </maskelement>
            <maskelement>
                <mename>generic</mename>
                <mevalue>6</mevalue>
            </maskelement>
            <maskelement>
                <mename>specific</mename>
                <mevalue>1</mevalue>
            </maskelement>
        </mask>
        <uei>uei.opennms.org/vendor/Juniper/traps/jnxFruNotifMismatch</uei>
        <event-label>JUNIPER-FRU-MIB defined trap event: jnxFruNotifMismatch</event-label>
        <descr>&lt;p&gt;A jnxFruInsertion trap signifies that the SNMP
                entity, acting in an agent role, has detected that
                the specified FRU (Field Replaceable Unit)  
                inserted into the chassis does not match what was
                configured.&lt;/p&gt;&lt;table&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	jnxFruCfgContentsIndex&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#1]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	jnxFruCfgL1Index&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#2]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	jnxFruCfgL2Index&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#3]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	jnxFruCfgL3Index&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#4]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	jnxFruCfgType&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#5]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;</descr>
        <logmsg dest="logndisplay">&lt;p&gt;
	jnxFruNotifMismatch trap received
	jnxFruCfgContentsIndex=%parm[#1]%
	jnxFruCfgL1Index=%parm[#2]%
	jnxFruCfgL2Index=%parm[#3]%
	jnxFruCfgL3Index=%parm[#4]%
	jnxFruCfgType=%parm[#5]%&lt;/p&gt;</logmsg>
        <severity>Critical</severity>
        <alarm-data reduction-key="%uei%:%dpname%:%nodeid%:%parm[#1]%:%parm[#2]%:%parm[#3]%:%parm[#4]%:%parm[#5]%" alarm-type="1" auto-clean="true"/>
    </event>
    <event>
        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.2636.4.23</mevalue>
            </maskelement>
            <maskelement>
                <mename>generic</mename>
                <mevalue>6</mevalue>
            </maskelement>
            <maskelement>
                <mename>specific</mename>
                <mevalue>2</mevalue>
            </maskelement>
        </mask>
        <uei>uei.opennms.org/vendor/Juniper/traps/jnxFruNotifAdminStatus</uei>
        <event-label>JUNIPER-FRU-MIB defined trap event: jnxFruNotifAdminStatus</event-label>
        <descr>&lt;p&gt;Notification of the Administrative state of the otn interface&lt;/p&gt;&lt;table&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	jnxFruCfgContentsIndex&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#1]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	jnxFruCfgL1Index&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#2]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	jnxFruCfgL2Index&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#3]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	jnxFruCfgL3Index&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#4]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	jnxFruCfgAdminState&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#5]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;
		inService(1)
		outOfService(2)
	&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;</descr>
        <logmsg dest="logndisplay">&lt;p&gt;
	jnxFruNotifAdminStatus trap received
	jnxFruCfgContentsIndex=%parm[#1]%
	jnxFruCfgL1Index=%parm[#2]%
	jnxFruCfgL2Index=%parm[#3]%
	jnxFruCfgL3Index=%parm[#4]%
	jnxFruCfgAdminState=%parm[#5]%&lt;/p&gt;</logmsg>
        <severity>Normal</severity>
        <varbindsdecode>
            <parmid>parm[#5]</parmid>
            <decode varbindvalue="1" varbinddecodedstring="inService"/>
            <decode varbindvalue="2" varbinddecodedstring="outOfService"/>
        </varbindsdecode>
        <alarm-data reduction-key="%uei%:%dpname%:%nodeid%:%parm[#1]%:%parm[#2]%:%parm[#3]%:%parm[#4]%:%parm[#5]%" alarm-type="1" auto-clean="true"/>
        <!-- <alarm-data reduction-key="%uei%:%dpname%:%nodeid%" alarm-type="1" auto-clean="true"/> -->
    </event>
