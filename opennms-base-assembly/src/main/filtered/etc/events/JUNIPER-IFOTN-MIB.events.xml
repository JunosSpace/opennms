    <uei>uei.opennms.org/vendor/Juniper/traps/jnxIfOtnNotificationAdminStatus</uei>
    <event-label>JUNIPER-IFOTN-MIB defined trap event: jnxIfOtnNotificationAdminStatus</event-label>
    <descr>
 Normal Notification of the admin state of the otn interface. AdminState=%parm[#2]% ifDescr=%parm[#1]% ifIndex=%parm[name-#1.-1]% 
	</descr>
    <logmsg dest="logndisplay">&lt;p&gt;
	jnxIfOtnNotificationAdminStatus trap received
	ifDescr=%parm[#1]%
	ifIndex=%parm[name-#1.-1]%
	jnxIfOtnAdminState=%parm[#2]%&lt;/p&gt;
	</logmsg>
    <severity>Normal</severity>
    <varbindsdecode>
      <parmid>parm[#2]</parmid>
      <decode varbindvalue="1" varbinddecodedstring="jnxAdminStateInService" />
      <decode varbindvalue="2" varbinddecodedstring="jnxAdminStateInServiceMA" />
      <decode varbindvalue="3" varbinddecodedstring="jnxAdminStateOutofService" />
      <decode varbindvalue="4" varbinddecodedstring="jnxAdminStateOutofServiceMA" />
    </varbindsdecode>
    <alarm-data reduction-key="%uei%:%dpname%:%nodeid%:%parm[name-#1.-1]%:%parm[#1]%:%parm[#2]%" alarm-type="1" auto-clean="true" />
